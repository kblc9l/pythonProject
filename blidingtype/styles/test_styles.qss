@font-face {
    font-family: "Noto_Sans_Regular";
    src: url("fonts/NotoSans-Regular.ttf") format("truetype");
    font-style: normal;
    font-weight: normal;
}

@font-face {
    font-family: "Noto_Sans_ExtraBold";
    src: url("fonts/NotoSans-ExtraBold.ttf") format("truetype");
    font-style: normal;
    font-weight: 800;
}

*{
    font-family: 'Noto_Sans_Regular';
    padding: 0;
    margin: 0;
    box-sizing: border-box;
    color: background250;
}

#Form{
    background-color: background1000;
}

#keyboard QPushButton{
    background-color: background1000;
}

#line_1 QPushButton{
    background-color: background1000;
}

#keyboard QPushButton{
    background-color: background1000;
    border: 1px solid background-dominant2;
    border-radius: 5px;
